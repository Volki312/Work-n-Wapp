{
    "manifest_version": 1,
    "name": "Work-n-Wapp",
    "version": "1.0",
  
    "description": "Mozilla add-on that lets you integrate whatsapp into your work environment",
  
    "icons": {
      "48": "icons/Work-n-Wapp-48.png",
      "96": "icons/Work-n-Wapp-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["wappify.js"]
      }
    ]
  
  }